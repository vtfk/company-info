    "url": "git+https://github.com/vtfk/company-info.git"
  },
  "bugs": {
    "url": "https://github.com/vtfk/company-info/issues"
  },
  "homepage": "https://github.com/vtfk/company-info/issues#readme",
  "license": "MIT",
  "devDependencies": {
    "coveralls": "3.0.6",
    "excel-as-json": "2.0.2",
    "standard": "14.3.1",
    "tap": "14.6.9"
  },
  "dependencies": {
    "@mcabreradev/filter": "^1.3.5"
  }
